source ~/.gdb/helpers/kde-devel-gdb
source ~/.gdb/helpers/funcionesH264
source ~/.gdb/helpers/extra
source ~/.gdb/safe_path


# Prompt "gdb$"  in green
#set prompt \033[32mgdb$ \033[0m


# Remember history over restarts
set history save
set history filename ~/.gdb_history




set print pretty on
set print array on 
set print demangle on 
set print object on 
set print vtbl on 



